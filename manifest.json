{
  "manifest_version": 2,
  "name": "Address Tools",
  "version": "1.0",
  "description": "显IP，生成二维码",
  "icons": {
    "16": "Images/16.png",
    "48": "Images/48.png",
    "128": "Images/128.png"
  },
  "default_locale": "zh_CN",
  "background": {
    "page": "back.html",
    "persistent": true
  },
  "browser_action": {
    "default_title": "Address Tools",
    "default_icon": "Images/16.png",
    "name": "browser_action_name"
  },
  "omnibox": {
    "keyword": "Address",
    "icons": "Images/16.png"
  },
  "permissions": [
    "activeTab",
    "browsingData",
    "contextMenus",
    "http://*/*",
    "https://*/*",
    "tabs",
    "webRequest"
  ],
  "web_accessible_resources": [
    "Images/*.png",
    "/libs/setQR.js"
  ]
}
